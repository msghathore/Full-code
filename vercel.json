{
  "version": 2,
  "name": "zavira-salon",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "env": {
    "VITE_SUPABASE_URL": "https://stppkvkcjsyusxwtbaej.supabase.co",
    "VITE_SUPABASE_PUBLISHABLE_KEY": "eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZSIsInJlZiI6InN0cHBrdmtjanN5dXN4d3RiYWVqIiwicm9sZSI6ImFub24iLCJpYXQiOjE3NjMyMjk4MTUsImV4cCI6MjA3ODgwNTgxNX0.sH9es8xu2tZlkhQrfaPcaYTAC8t6CjrI7LL9BKfT-v0",
    "VITE_SQUARE_APPLICATION_ID": "sq0idp-_h9VMgdBekjwqCkLMetFHg",
    "VITE_SQUARE_LOCATION_ID": "LKH6TY590G319",
    "VITE_SQUARE_ENVIRONMENT": "production",
    "VITE_CLIENT_CLERK_PUBLISHABLE_KEY": "pk_test_d2VsY29tZWQtY3JhbmUtMTUuY2xlcmsuYWNjb3VudHMuZGV2JA",
    "VITE_STAFF_CLERK_PUBLISHABLE_KEY": "pk_test_dmlhYmxlLWJlYWdsZS00LmNsZXJrLmFjY291bnRzLmRldiQ",
    "VITE_APP_ENV": "production",
    "VITE_APP_URL": "https://zavira.ca"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-Requested-With, Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/appointment",
      "destination": "/booking",
      "permanent": true
    },
    {
      "source": "/www.zavira.ca",
      "destination": "https://zavira.ca",
      "permanent": true
    }
  ]
}